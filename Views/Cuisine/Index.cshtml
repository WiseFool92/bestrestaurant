@{
  Layout = "_Layout";
}

@using TableData.Models;
@model List<TableData.Models.Cuisine>;

<h1>Cuisine</h1>

@if (@Model.Count == 0)
{
  <h3>No cuisine categories have been added!  Create the first one!</h3>
}

<ul>
  @foreach (Cuisine cuisine in Model)
  {
    <li>@Html.ActionLink($"{cuisine.Type}", "Details", new { id = cuisine.CuisineId }) </li>
  }
</ul>